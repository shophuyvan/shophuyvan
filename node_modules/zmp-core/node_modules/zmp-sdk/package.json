{
  "name": "zmp-sdk",
  "version": "1.8.0",
  "description": "Zalo Mini App SDK",
  "license": "MIT",
  "main": "dist/index.js",
  "browser": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "umdName": "zmp",
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "docs": "rimraf ./dist/docs && auto-changelog -p && jsdoc -c ./jsdoc.js",
    "tsdocs": "rimraf ./tsdocs && typedoc ./src/index.ts --options ./typedoc.json ./src",
    "watch": "rollup -c -w",
    "build": "rimraf dist && NODE_ENV=production rollup -c",
    "version": "NODE_ENV=production yarn run build && yarn run docs && yarn run tsdocs && git add docs/ docs-template/ tsdocs/",
    "release": "np --no-2fa"
  },
  "author": {
    "name": "Zalo"
  },
  "np": {
    "yarn": true,
    "runPublish": true
  },
  "auto-changelog": {
    "output": "./docs-template/changelog/changelog.md",
    "unreleased": true,
    "commitLimit": false,
    "template": "./docs-template/changelog-template.hbs",
    "handlebarsSetup": "./docs-template/changelog-setup.js"
  },
  "apidoc": {
    "title": "Custom apiDoc browser title",
    "url": ""
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.0.0-beta.46",
    "@open-wc/rollup-plugin-html": "^1.2.5",
    "@rollup/plugin-html": "^0.2.2",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "jest": "^25.1.0",
    "kida-docs": "^2.5.3",
    "np": "^7.2.0",
    "npm": "^6.14.11",
    "rimraf": "^3.0.0",
    "rollup": "^2.0.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-bundle-html": "^0.2.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-uglify": "^6.0.3",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.4.1",
    "tslib": "^2.0.0",
    "typedoc": "^0.20.20",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3",
    "yarn": "^1.22.0",
    "zalo-js-bridge": "^1.10.19"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.21"
  }
}
